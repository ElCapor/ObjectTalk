// Function class test

function test(s, f) {
	return f(s)
}

function lower(s) {
	return s.lower();
}

function upper(s) {
	return s.upper();
}

assert('test("Hello", lower) == "hello"')
assert('test("Hello", upper) == "HELLO"')

trim = function(s) {
	return s.trim()
}

assert('trim(" test  ") == "test"')

// Well done
print("Passed all function tests")
