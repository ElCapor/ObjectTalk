// Dict class test
dict = Dict("name", "value", "test", true, "result", 2 + 3 + 5)
assert(dict.size() == 3)
assert(dict["result"] == 10)

dict["pi"] = 3.14
assert(dict.size() == 4)
assert(dict["pi"] == 3.14)

dict2 = dict.clone()
assert(dict2.size() == 4)
dict2.erase("pi")
assert(dict2.size() == 3)
assert(dict.size() == 4)

dict3 = { "name": "value", "flag": true, "result": (10 - 4) / 3 }
assert(dict3.size() == 3)
assert(dict3["result"] == 2)

// Well done
print("Passed all dictionary tests")
